<h1><%= @project.title %> <small>(<%=@project.category%>)</small></h1><small><%= @project.date %></small>
<div class='row'>
  <div class='col-md-8'>
    <p><%= @project.notes %> </p>
  </div>
  <div class='col-md-4'>
    <%= link_to "Edit Project", edit_project_path(@project), class: 'btn btn-success' %>
    <%= link_to "Delete Project", [@project], method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure you wish to delete this project?'} %>
  </div>
</div>
